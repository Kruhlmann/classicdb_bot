{
  "name": "classicdb_bot",
  "version": "1.2.3",
  "description": "A discord bot, which makes it easy to link items from the WoW Classic database on classicdb.ch",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run build && node dist/src/main.js",
    "test": "mocha -r ts-node/register test/**/*.spec.ts --exit",
    "coverage": "nyc npm run test",
    "lint": "tslint src/**/*.ts",
    "fix": "tslint src/**/*.ts",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kruhlmann/classicdb_bot.git"
  },
  "author": "Andreas Kruhlmann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kruhlmann/classicdb_bot/issues"
  },
  "homepage": "https://github.com/Kruhlmann/classicdb_bot#readme",
  "devDependencies": {
    "@sentry/node": "^4.6.1",
    "@types/cheerio": "^0.22.11",
    "@types/request-promise": "^4.1.43",
    "bufferutil": "^3.0.3",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.2",
    "discord.js": "^11.4.2",
    "erlpack": "github:discordapp/erlpack",
    "libsodium-wrappers": "^0.7.4",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "node-opus": "^0.2.7",
    "opusscript": "^0.0.6",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "uws": "^9.14.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "nyc": "^14.1.0",
    "source-map-support": "^0.5.12"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
